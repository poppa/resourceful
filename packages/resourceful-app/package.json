{
  "name": "@poppanator/resourceful-app",
  "scripts": {
    "start": "electron .",
    "dev:app": "yarn clean:app && tsc -w",
    "dev:ui": "yarn clean:ui && parcel watch -d ./dist/ui ./src/ui/app.html --public-url .",
    "build:app": "yarn clean:app && tsc -p ./tsconfig.json",
    "build:ui": "yarn clean:ui && parcel build -d ./dist/ui ./src/ui/app.html --public-url .",
    "clean": "rimraf dist",
    "clean:app": "rimraf dist/app",
    "clean:ui": "rimraf dist/ui"
  },
  "private": true,
  "version": "0.0.1",
  "main": "dist/app/main.js",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "deepmerge": "^4.2.2",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "tslib": "^1.11.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "sass": "^1.26.5"
  },
  "build": {
    "appId": "org.poppanator.resourceful",
    "mac": {
      "category": ""
    }
  },
  "app": {
    "dest": "./dist",
    "srcPaths": {
      "scss": "src/scss/**/*.scss",
      "img": "src/img/**/*.[png,svg]",
      "html": "src/**/*.html"
    }
  }
}
